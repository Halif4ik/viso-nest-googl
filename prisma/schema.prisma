generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Row {
  id            Int      @id @default(autoincrement())
  row_sheets    Int
  column_sheets String
  text          String
  empty         Boolean  @default(false)
  createdAt     DateTime @default(now())
}
